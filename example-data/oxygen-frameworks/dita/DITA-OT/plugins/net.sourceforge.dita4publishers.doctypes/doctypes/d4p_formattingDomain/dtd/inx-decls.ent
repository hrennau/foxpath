<?xml version="1.0" encoding="UTF-8"?>
<!--=====================================
    INX Element Types
    
    Declares elements types for INX elements so documents
    that include them can validate.
    
    Note that it is not possible to have a complete
    DTD for INX files since plugins can add new
    object types or attributes.
    =====================================-->
    
    
 <!ENTITY % inx-components
   "(txsr |
     cflo)
   "
 >
 
 <!ELEMENT txsr
   (pcnt)
 >
