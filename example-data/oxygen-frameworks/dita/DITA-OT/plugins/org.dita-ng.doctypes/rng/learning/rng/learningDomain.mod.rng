    </element>
  </define>
  <define name="lcSequencing.attlist" combine="interleave">
    <ref name="lcSequencing.attributes"/>
  </define>
  
  <define name="lcMatching.content">
    <optional>
      <ref name="title"/>
    </optional>
    <ref name="lcQuestion"/>
    <optional>
      <ref name="lcAsset"/>
    </optional>
    <ref name="lcMatchTable"/>
    <optional>
      <ref name="lcFeedbackIncorrect"/>
    </optional>
    <optional>
      <ref name="lcFeedbackCorrect"/>
    </optional>
    <zeroOrMore>
      <ref name="data"/>
