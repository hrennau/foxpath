      <ref name="lcFeedbackCorrect.content"/>
    </element>
  </define>
  <define name="lcFeedbackCorrect.attlist" combine="interleave">
    <ref name="lcFeedbackCorrect.attributes"/>
  </define>
  
  <define name="lcFeedbackIncorrect.content">
    <zeroOrMore>
