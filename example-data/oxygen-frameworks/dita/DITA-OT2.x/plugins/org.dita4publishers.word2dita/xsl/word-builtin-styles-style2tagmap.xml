<?xml version="1.0" encoding="UTF-8"?>
<style2tagmap xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xmlns="urn:public:dita4publishers.org:namespaces:word2dita:style2tagmap"
              xsi:schemaLocation="urn:public:dita4publishers.org:namespaces:word2dita:style2tagmap ../../org.dita4publishers.doctypes/doctypes/style2tagmap/xsd/style2tagmap.xsd">
  <title>Word Default Styles Style-to-Tag Map</title>
  <documentation> </documentation>
  <styles>
      <output mapType="bookmap"
              prologType="topicmeta"
              name="bookmap"
              doctype-system="bookmap.dtd"
              doctype-public="-//OASIS//DTD DITA BookMap//EN"/>

      <output topicType="concept"
              bodyType="conbody"
              name="concept"
              doctype-system="concept.dtd"
              doctype-public="-//OASIS//DTD DITA Concept//EN"/>

      <output name="task"
              doctype-system="task.dtd"
              doctype-public="-//OASIS//DTD DITA Task//EN"/>

      <output name="reference"
              doctype-system="reference.dtd"
              doctype-public="-//OASIS//DTD DITA Reference//EN"/>

      <paragraphStyle styleName="Title"
                      level="0"
                      structureType="mapTitle"
                      tagName="title">
         <mapProperties format="bookmap" prologType="topicmeta" tagName="title"/>
      </paragraphStyle>


      <paragraphStyle styleName="Heading 1"
                      structureType="topicTitle"
                      tagName="title"
                      topicZone="body"
                      level="1">
         <topicrefProperties topicrefType="chapter"/>
         <topicProperties topicDoc="yes"
                          topicType="concept"
                          bodyType="conbody"
                          format="concept"/>
      </paragraphStyle>

      <paragraphStyle styleName="Heading 2"
                      structureType="topicTitle"
                      tagName="title"
                      topicZone="body"
                      level="2">
         <topicrefProperties topicrefType="topicref"/>
         <topicProperties topicDoc="yes"
                          topicType="concept"
                          bodyType="conbody"
                          format="concept"/>
      </paragraphStyle>
    
      <paragraphStyle styleName="Heading 3"
                      structureType="topicTitle"
                      tagName="title"
                      topicZone="body"
                      level="3">
         <topicrefProperties topicrefType="topicref"/>
         <topicProperties topicDoc="yes"
                          topicType="concept"
                          bodyType="conbody"
                          format="concept"/>
      </paragraphStyle>
    
      <paragraphStyle styleName="Normal" tagName="p" topicZone="body" level="1"/>

      <paragraphStyle styleName="Default Paragraph Font"
                      tagName="ph"
                      topicZone="body"
                      level="1"/>

      <paragraphStyle styleName="Cover Page Name"
                      tagName="p"
                      topicZone="body"
                      level="1"/>
      <paragraphStyle styleName="Cover Page Name Char"
                      tagName="ph"
                      topicZone="body"
                      level="1"/>

      <paragraphStyle styleName="Heading1Char"
                      tagName="ph"
                      topicZone="body"
                      level="1"/>
      <paragraphStyle styleName="TitleChar" tagName="ph" topicZone="body" level="1"/>

      <paragraphStyle styleName="Heading2Char"
                      tagName="ph"
                      topicZone="body"
                      level="1"/>

      <paragraphStyle styleName="TOC 1" structureType="skip"/>
      <paragraphStyle styleName="TOC 2" structureType="skip"/>
      <paragraphStyle styleName="TOC 3" structureType="skip"/>
      <paragraphStyle styleName="TOC 4" structureType="skip"/>
      <paragraphStyle styleName="TOC 5" structureType="skip"/>
      <paragraphStyle styleName="TOC 6" structureType="skip"/>
      <paragraphStyle styleName="TOC 7" structureType="skip"/>
      <paragraphStyle styleName="TOC 8" structureType="skip"/>
      <paragraphStyle styleName="TOC 9" structureType="skip"/>

      <paragraphStyle styleName="Heading3Char"
                      tagName="ph"
                      topicZone="body"
                      level="1"/>
      <paragraphStyle styleName="Heading4Char"
                      tagName="ph"
                      topicZone="body"
                      level="1"/>

      <paragraphStyle styleName="List Bullet"
                      containerType="ul"
                      tagName="li"
                      level="1"
                      structureType="block"
                      topicZone="body"/>
      <paragraphStyle styleName="Body Text Indent"
                      tagName="p"
                      topicZone="body"
                      level="1"/>
      <paragraphStyle styleName="BodyTextIndentChar"
                      tagName="ph"
                      topicZone="body"
                      level="1"/>
      <paragraphStyle styleName="List Bullet 2"
                      containerType="ul"
                      tagName="li"
                      level="2"
                      structureType="block"
                      topicZone="body"/>
      <paragraphStyle styleName="Block Text"
                      tagName="p"
                      topicZone="body"
                      outputclass="boxed"
                      level="1"/>
      <paragraphStyle styleName="Body Text" tagName="p" topicZone="body" level="1"/>
      <paragraphStyle styleName="Normal" tagName="p" topicZone="body" level="1"/>
      <paragraphStyle styleName="BodyTextChar"
                      tagName="ph"
                      topicZone="body"
                      level="1"/>
      <paragraphStyle styleName="List Number"
                      containerType="ol"
                      tagName="li"
                      level="1"
                      structureType="block"
                      topicZone="body"/>
      <paragraphStyle styleName="List Number 2"
                      containerType="ol"
                      tagName="li"
                      level="2"
                      structureType="block"
                      topicZone="body"/>
    
      <characterStyle styleName="Hyperlink" structureType="xref"/>


  </styles>
</style2tagmap>
