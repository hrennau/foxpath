  [ a:defaultValue = "- topic/ul " ] attribute class { text }?
unknown.attlist &=
  global-atts,
  [ a:defaultValue = "- topic/unknown " ] attribute class { text }?
xref.attlist &=
  global-atts,
  [ a:defaultValue = "- topic/xref " ] attribute class { text }?
