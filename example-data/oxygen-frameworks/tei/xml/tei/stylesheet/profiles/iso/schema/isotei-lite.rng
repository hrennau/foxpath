          <data type="gDay"/>
          <data type="gYearMonth"/>
          <data type="gMonthDay"/>
          <data type="time"/>
          <data type="dateTime"/>
          <data type="token">
            <param name="pattern">[0-9.,DHMPRSTWYZ/:+\-]+</param>
