      <xs:annotation>
        <xs:documentation>indicates the end of a span initiated by the element bearing this attribute.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.styleDef.attributes">
