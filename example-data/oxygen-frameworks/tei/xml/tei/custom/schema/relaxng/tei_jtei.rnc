             ".\x{a}" ~
             "                  "
           ]
           "\x{a}" ~
           "                "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "tei_jtei-graphic-jtei.sch-graphic-context-constraint-rule-20"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:graphic"
           "\x{a}" ~
           "                  "
           sch:assert [
             test = "parent::tei:figure"
             "\x{a}" ~
             "                    "
             sch:name [ ]
             " may only occur inside figure.\x{a}" ~
             "                  "
           ]
           "\x{a}" ~
           "                "
         ]
         "\x{a}" ~
         "         "
       ],
    att.global.attributes,
    att.media.attributes,
    att.resourced.attributes,
    empty
  }
