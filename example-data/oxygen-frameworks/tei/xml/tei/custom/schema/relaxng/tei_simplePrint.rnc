      name = "results"
      value =
        "for $t in  tokenize(normalize-space(@target),'\s+') return starts-with($t,'#') and not(id(substring($t,2)))"
    ]
    "\x{a}" ~
    "                        "
    sch:report [
      test = "some $x in $results  satisfies $x"
      " Error: Every local pointer\x{a}" ~
      '                          in "'
