            <zeroOrMore>
               <choice>
          
                  <ref name="model.divTop"/>
          
          
                  <ref name="model.global"/>
          
               </choice>
            </zeroOrMore>
      
            <choice>
               <oneOrMore>
                  <group>
                     <ref name="item"/>
          
                     <zeroOrMore>
                        <ref name="model.global"/>
                     </zeroOrMore>
          
                  </group>
               </oneOrMore>
               <group>
          
            
          
          
            
          
                  <oneOrMore>
                     <group>
                        <ref name="label"/>
            
                        <zeroOrMore>
                           <ref name="model.global"/>
                        </zeroOrMore>
            
                        <ref name="item"/>
            
                        <zeroOrMore>
                           <ref name="model.global"/>
                        </zeroOrMore>
            
                     </group>
                  </oneOrMore>
               </group>
            </choice>
      
            <zeroOrMore>
               <group>
          
                  <ref name="model.divBottom"/>
          
          
                  <zeroOrMore>
                     <ref name="model.global"/>
                  </zeroOrMore>
          
               </group>
