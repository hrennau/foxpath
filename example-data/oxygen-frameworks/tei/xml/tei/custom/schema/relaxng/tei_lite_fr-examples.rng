            <zeroOrMore>
               <choice>
                  <ref name="model.headLike"/>
                  <ref name="model.global"/>
               </choice>
            </zeroOrMore>
      
            <choice>
               <oneOrMore>
                  <group>
                     <ref name="row"/>
          
                     <zeroOrMore>
                        <ref name="model.global"/>
                     </zeroOrMore>
          
                  </group>
               </oneOrMore>
               <oneOrMore>
                  <group>
          
                     <ref name="model.graphicLike"/>
          
          
                     <zeroOrMore>
                        <ref name="model.global"/>
                     </zeroOrMore>
          
                  </group>
               </oneOrMore>
            </choice>
            <zeroOrMore>
               <group>
        
                  <ref name="model.divBottom"/>
        
        
                  <zeroOrMore>
                     <ref name="model.global"/>
                  </zeroOrMore>
        
               </group>
